#tag Module
Protected Module OffsetExtension
	#tag Method, Flags = &h0
		Function toUIOffset(extends u as UIOffset32Bit) As UIOffset
		  dim result as UIOffset
		  result.Horizontal = u.Horizontal
		  result.Vertical = u.Vertical
		  return result
		End Function
	#tag EndMethod

	#tag Method, Flags = &h0
		Function toUIOffset32(extends u as UIOffset) As UIOffset32Bit
		  dim result as UIOffset32Bit
		  result.Horizontal = u.Horizontal
		  result.Vertical = u.Vertical
		  return result
		End Function
	#tag EndMethod

	#tag Method, Flags = &h0
		Function UIOffset(Horizontal as Double, Vertical As Double) As UIOffset
		  dim result as UIOffset
		  result.Horizontal = Horizontal
		  result.Vertical = Vertical
		  return result
		End Function
	#tag EndMethod

	#tag Method, Flags = &h0
		Function UIOffsetZero() As UIOffset
		  dim result as UIOffset
		  return result
		End Function
